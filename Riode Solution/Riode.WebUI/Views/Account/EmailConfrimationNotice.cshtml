
@{
    var m = ViewBag.message as Tuple<bool, string>;
}


<nav class="breadcrumb-nav">
    <div class="container">
        <ul class="breadcrumb">
            <li><a asp-controller="home" asp-action="index"><i class="d-icon-home"></i></a></li>
            <li>Contact Us</li>
        </ul>
    </div>
</nav>
<div class="page-content mt-10 pt-7  mb-10">
    <section class="contact-section">
        <div class="container">
            <div class="row d-flex justify-content-center">
                <div class="col-8 alert alert-success text-white">
                    <p>Check your email, confirm it and login to the website!</p>
                </div>
            </div>
        </div>
    </section>
</div>

@section addjs{
    <script>
        $(document).ready(function () {
            setTimeout("window.location.pathname = \"home/index\";", 5000);
        })
    </script>
}

